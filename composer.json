{
  "name": "contributte/deployer-extension",
  "description": "Ftp-Deployment Extension for Nette",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/deployer-extension",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=7.2",
    "nette/di": "^3.0.2",
    "dg/ftp-deployment": "^3.3.1"
  },
  "require-dev": {
    "ninjify/qa": "^0.12",
    "ninjify/nunjuck": "^0.4",
    "mockery/mockery": "^1.3.0",
    "phpstan/phpstan": "^0.12.0",
    "phpstan/phpstan-deprecation-rules": "^0.12.0",
    "phpstan/phpstan-nette": "^0.12.0",
    "phpstan/phpstan-strict-rules": "^0.12.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Deployer\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "3.3.x-dev"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
